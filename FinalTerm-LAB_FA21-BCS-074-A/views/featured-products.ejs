<h1>Featured Products</h1>
  <ul>
    <% products.forEach(product => { %>
      <li>
        <h2><a href="/products/<%= product._id %>"><%= product.name %></a></h2>
        <p><%= product.description %></p>
        <p>Price: $<%= product.price %></p>
        <p>Category: <%= product.category %></p>
      </li>
    <% }) %>
  </ul>